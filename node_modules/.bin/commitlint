#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wym/Downloads/workspace/react18/node_modules/.pnpm/commitlint@18.4.3_typescript@4.9.5/node_modules/commitlint/node_modules:/Users/wym/Downloads/workspace/react18/node_modules/.pnpm/commitlint@18.4.3_typescript@4.9.5/node_modules:/Users/wym/Downloads/workspace/react18/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wym/Downloads/workspace/react18/node_modules/.pnpm/commitlint@18.4.3_typescript@4.9.5/node_modules/commitlint/node_modules:/Users/wym/Downloads/workspace/react18/node_modules/.pnpm/commitlint@18.4.3_typescript@4.9.5/node_modules:/Users/wym/Downloads/workspace/react18/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../commitlint/cli.js" "$@"
else
  exec node  "$basedir/../commitlint/cli.js" "$@"
fi
